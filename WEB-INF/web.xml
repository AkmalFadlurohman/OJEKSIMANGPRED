<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems,
Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/j2ee/dtds/web-app_2_3.dtd">

<web-app>
	<listener>
		<listener-class>
			com.sun.xml.ws.transport.http.servlet.WSServletContextListener
		</listener-class>
	</listener>
	<servlet>
		<servlet-name>Hello</servlet-name>
		<servlet-class>Hello</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.ojeksimangpred.IDServices.Login</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>Register</servlet-name>
		<servlet-class>com.ojeksimangpred.IDServices.Register</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>AccessManager</servlet-name>
		<servlet-class>com.ojeksimangpred.IDServices.AccessManager</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>UserManager</servlet-name>
		<servlet-class>com.ojeksimangpred.IDServices.UserManager</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>ImageRetriever</servlet-name>
		<servlet-class>com.ojeksimangpred.IDServices.ImageRetriever</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>OjolServices/aloha</servlet-name>
		<servlet-class>
			com.sun.xml.ws.transport.http.servlet.WSServlet
		</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Hello</servlet-name>
		<url-pattern>/IDServices/Hello</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/IDServices/Login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/IDServices/Register</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AccessManager</servlet-name>
		<url-pattern>/IDServices/AccessManager</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserManager</servlet-name>
		<url-pattern>/IDServices/UserManager</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ImageRetriever</servlet-name>
		<url-pattern>/IDServices/ImageRetriever</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>OjolServices/aloha</servlet-name>
		<url-pattern>/OjolServices/aloha</url-pattern>
	</servlet-mapping>
	
</web-app>
